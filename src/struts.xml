<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.devMode" value="true" />
	<constant name="struts.ui.theme" value="simple" />

	<package name="app" extends="struts-default">
		<result-types>
			<result-type name="tiles"
				class="org.apache.struts2.views.tiles.TilesResult" />
			<result-type name="image" class="net.sas.util.ImageBytesResult" />
		</result-types>

		<action name="ImageAction" class="net.sas.util.ImageAction">
			<result name="success" type="image"></result>
		</action>
	</package>

	<package name="employee" extends="app" namespace="/employee">
		<action name="list" class="net.sas.controller.EmployeeAction">
			<result name="input" type="tiles">/employee.tiles</result>
		</action>
		<action name="clear" class="net.sas.controller.EmployeeAction"
			method="clear">
			<result name="input" type="tiles">/employee.tiles</result>
		</action>
		<action name="view" class="net.sas.controller.EmployeeAction"
			method="view">
			<result name="input" type="tiles">/employee.tiles</result>
		</action>
		<action name="save" class="net.sas.controller.EmployeeAction"
			method="save">
			<result name="input" type="tiles">/employee.tiles</result>
		</action>
		<action name="delete" class="net.sas.controller.EmployeeAction"
			method="delete">
			<result name="input" type="tiles">/employee.tiles</result>
		</action>
		<action name="previous" class="net.sas.controller.EmployeeAction"
			method="previous">
			<result name="input" type="tiles">/employee.tiles</result>
		</action>
		<action name="next" class="net.sas.controller.EmployeeAction"
			method="next">
			<result name="input" type="tiles">/employee.tiles</result>
		</action>
		<action name="first" class="net.sas.controller.EmployeeAction"
			method="first">
			<result name="input" type="tiles">/employee.tiles</result>
		</action>
		<action name="last" class="net.sas.controller.EmployeeAction"
			method="last">
			<result name="input" type="tiles">/employee.tiles</result>
		</action>

		<action name="history">
			<result type="tiles">/employeeHistory.tiles</result>
		</action>
	</package>

	<package name="vehicle" extends="app" namespace="/vehicle">
		<action name="list" class="net.sas.controller.VehicleAction">
			<result name="input" type="tiles">/vehicle.tiles</result>
		</action>
		<action name="clear" class="net.sas.controller.VehicleAction"
			method="clear">
			<result name="input" type="tiles">/vehicle.tiles</result>
		</action>
		<action name="view" class="net.sas.controller.VehicleAction"
			method="view">
			<result name="input" type="tiles">/vehicle.tiles</result>
		</action>
		<action name="save" class="net.sas.controller.VehicleAction"
			method="save">
			<result name="input" type="tiles">/vehicle.tiles</result>
		</action>
		<action name="delete" class="net.sas.controller.VehicleAction"
			method="delete">
			<result name="input" type="tiles">/vehicle.tiles</result>
		</action>
		<action name="previous" class="net.sas.controller.VehicleAction"
			method="previous">
			<result name="input" type="tiles">/vehicle.tiles</result>
		</action>
		<action name="next" class="net.sas.controller.VehicleAction"
			method="next">
			<result name="input" type="tiles">/vehicle.tiles</result>
		</action>
		<action name="first" class="net.sas.controller.VehicleAction"
			method="first">
			<result name="input" type="tiles">/vehicle.tiles</result>
		</action>
		<action name="last" class="net.sas.controller.VehicleAction"
			method="last">
			<result name="input" type="tiles">/vehicle.tiles</result>
		</action>
	</package>

	<package name="supplier" extends="app" namespace="/supplier">
		<action name="list" class="net.sas.controller.SupplierAction">
			<result name="input" type="tiles">/supplier.tiles</result>
		</action>
		<action name="clear" class="net.sas.controller.SupplierAction"
			method="clear">
			<result name="input" type="tiles">/supplier.tiles</result>
		</action>
		<action name="view" class="net.sas.controller.SupplierAction"
			method="view">
			<result name="input" type="tiles">/supplier.tiles</result>
		</action>
		<action name="save" class="net.sas.controller.SupplierAction"
			method="save">
			<result name="input" type="tiles">/supplier.tiles</result>
		</action>
		<action name="delete" class="net.sas.controller.SupplierAction"
			method="delete">
			<result name="input" type="tiles">/supplier.tiles</result>
		</action>
		<action name="previous" class="net.sas.controller.SupplierAction"
			method="previous">
			<result name="input" type="tiles">/supplier.tiles</result>
		</action>
		<action name="next" class="net.sas.controller.SupplierAction"
			method="next">
			<result name="input" type="tiles">/supplier.tiles</result>
		</action>
		<action name="first" class="net.sas.controller.SupplierAction"
			method="first">
			<result name="input" type="tiles">/supplier.tiles</result>
		</action>
		<action name="last" class="net.sas.controller.SupplierAction"
			method="last">
			<result name="input" type="tiles">/supplier.tiles</result>
		</action>
	</package>
</struts>