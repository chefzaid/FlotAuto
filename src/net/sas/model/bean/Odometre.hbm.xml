<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="net.sas.model.bean">
	<class name="Adresse">
		<id name="id" type="long">
			<generator class="native" />
		</id>
		<many-to-one name="examenSante" class="ExamenSante"
			unique="true" cascade="save-update, delete, delete-orphan" />
		<property name="compteur" type="integer" />
		<property name="dateLecture" type="date" />
		<property name="notes" type="string" />
	</class>
</hibernate-mapping>